def palindrome_checker (number):
    original_number = number
    reversed_number = 0
    while number > 0 :
        remainder = number % 10
        reversed_number =  (reversed_number * 10) + remainder 
        number = number//10   
    if original_number == reversed_number:
         print ("this is a palindrome")
    else:
         print ("this is not a palindrome")
         
         
         

         
